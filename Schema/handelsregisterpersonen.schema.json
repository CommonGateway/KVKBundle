{
    "title": "handelsregisterpersonen",
    "$id": "https://opencatalogi.nl/handelsregisterpersonen.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "version": "0.1.0",
    "description": "Zoeken van handelsregisterpersonen (een natuurlijk persoon die niet is ingeschreven in de Basisregistratie Personen).",
    "type": "object",
    "properties": {
        "naam": {
            "type": "string"
        },
        "woonplaats": {
            "type": "string"
        },
        "straatnaam": {
            "type": "string"
        },
        "huisnummer": {
            "type": "integer",
            "minLength": "1"
        },
        "postcode": {
            "type": "string",
            "example": "1234AZ",
            "format": "^[1-9]{1}[0-9]{3}[A-Za-z]{2}$"
        }
    }
}